    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#EfficientModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#EfficientModel">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#hasComplexity"/>
                        <owl:hasValue>Low</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#hasTrainingTime"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">60.0</xsd:maxInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Model wydajny: niska złożoność i krótki czas treningu</rdfs:comment>
        <rdfs:label>Efficient Model</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#InterpretableModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#InterpretableModel">

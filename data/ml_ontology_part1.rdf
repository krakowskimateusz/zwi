<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/2024/ml-selection#"
     xml:base="http://www.semanticweb.org/ontologies/2024/ml-selection"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection">
        <rdfs:comment>Ontologia doboru modeli uczenia maszynowego - Część 1: Modele i Algorytmy</rdfs:comment>
        <rdfs:label>ML Model Selection Ontology - Part 1</rdfs:label>
        <owl:versionInfo>1.0 - Models and Algorithms Component</owl:versionInfo>
    </owl:Ontology>
    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#isSuitableFor -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#isSuitableFor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLTask"/>
        <rdfs:comment>Określa, do jakiego typu zadania ML model jest odpowiedni</rdfs:comment>
        <rdfs:label>is suitable for</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#requiresPreprocessing -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#requiresPreprocessing">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique"/>
        <rdfs:comment>Wskazuje technike preprocessingu wymaganą przez model</rdfs:comment>
        <rdfs:label>requires preprocessing</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#isAppliedTo -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#isAppliedTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Dataset"/>
        <rdfs:comment>Model jest zastosowany do konkretnego zbioru danych</rdfs:comment>
        <rdfs:label>is applied to</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#performsWellOn -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#performsWellOn">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#isAppliedTo"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Dataset"/>
        <rdfs:comment>Model osiąga dobre wyniki na danym zbiorze danych</rdfs:comment>
        <rdfs:label>performs well on</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#hasHyperparameter -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#hasHyperparameter">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Hyperparameter"/>
        <rdfs:comment>Model ma określony hiperparametr</rdfs:comment>
        <rdfs:label>has hyperparameter</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#isEvaluatedBy -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#isEvaluatedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PerformanceMetric"/>
        <rdfs:comment>Model jest oceniany za pomocą konkretnej metryki</rdfs:comment>
        <rdfs:label>is evaluated by</rdfs:label>
    </owl:ObjectProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#hasTrainingTime -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#hasTrainingTime">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Średni czas treningu modelu w minutach</rdfs:comment>
        <rdfs:label>has training time</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#hasComplexity -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#hasComplexity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Złożoność obliczeniowa modelu: Low, Medium, High</rdfs:comment>
        <rdfs:label>has complexity</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#supportsParallelization -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#supportsParallelization">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Czy model wspiera równoległe przetwarzanie</rdfs:comment>
        <rdfs:label>supports parallelization</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#minSamplesRequired -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#minSamplesRequired">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Minimalna liczba próbek wymagana do efektywnego treningu</rdfs:comment>
        <rdfs:label>minimum samples required</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#handlesNonlinearity -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#handlesNonlinearity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Czy model potrafi obsługiwać nieliniowe zależności</rdfs:comment>
        <rdfs:label>handles nonlinearity</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#isInterpretable -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#isInterpretable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Czy model jest łatwo interpretowalny</rdfs:comment>
        <rdfs:label>is interpretable</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#memoryRequirement -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#memoryRequirement">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Wymagania pamięciowe: Low, Medium, High</rdfs:comment>
        <rdfs:label>memory requirement</rdfs:label>
    </owl:DatatypeProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#MLTask -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#MLTask">
        <rdfs:comment>Typ zadania uczenia maszynowego</rdfs:comment>
        <rdfs:label>ML Task</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#SupervisedLearning -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#SupervisedLearning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLTask"/>
        <rdfs:comment>Uczenie nadzorowane - model uczy się na podstawie etykietowanych danych</rdfs:comment>
        <rdfs:label>Supervised Learning</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#Classification -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Classification">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#SupervisedLearning"/>
        <rdfs:comment>Zadanie klasyfikacji - przypisywanie kategorii</rdfs:comment>
        <rdfs:label>Classification</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#BinaryClassification -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#BinaryClassification">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Classification"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MulticlassClassification"/>
        <rdfs:comment>Klasyfikacja binarna - dwie klasy</rdfs:comment>
        <rdfs:label>Binary Classification</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#MulticlassClassification -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#MulticlassClassification">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Classification"/>
        <rdfs:comment>Klasyfikacja wieloklasowa - więcej niż dwie klasy</rdfs:comment>
        <rdfs:label>Multiclass Classification</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#Regression -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Regression">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#SupervisedLearning"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Classification"/>
        <rdfs:comment>Zadanie regresji - przewidywanie wartości ciągłych</rdfs:comment>
        <rdfs:label>Regression</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#UnsupervisedLearning -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#UnsupervisedLearning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLTask"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#SupervisedLearning"/>
        <rdfs:comment>Uczenie nienadzorowane - bez etykiet</rdfs:comment>
        <rdfs:label>Unsupervised Learning</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#Clustering -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Clustering">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#UnsupervisedLearning"/>
        <rdfs:comment>Grupowanie danych w klastry</rdfs:comment>
        <rdfs:label>Clustering</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#DimensionalityReduction -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#DimensionalityReduction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#UnsupervisedLearning"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#Clustering"/>
        <rdfs:comment>Redukcja wymiarowości danych</rdfs:comment>
        <rdfs:label>Dimensionality Reduction</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel">
        <rdfs:comment>Model uczenia maszynowego</rdfs:comment>
        <rdfs:label>ML Model</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <rdfs:comment>Modele oparte na drzewach decyzyjnych</rdfs:comment>
        <rdfs:label>Tree-Based Model</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#DecisionTree -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#DecisionTree">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <rdfs:comment>Pojedyncze drzewo decyzyjne</rdfs:comment>
        <rdfs:label>Decision Tree</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#RandomForest -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#RandomForest">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <rdfs:comment>Ensemble drzew decyzyjnych</rdfs:comment>
        <rdfs:label>Random Forest</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#GradientBoosting -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#GradientBoosting">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <rdfs:comment>Gradient Boosting - sekwencyjne budowanie drzew</rdfs:comment>
        <rdfs:label>Gradient Boosting</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#XGBoost -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#XGBoost">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#GradientBoosting"/>
        <rdfs:comment>Extreme Gradient Boosting - zoptymalizowana wersja GB</rdfs:comment>
        <rdfs:label>XGBoost</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <rdfs:comment>Modele liniowe</rdfs:comment>
        <rdfs:label>Linear Model</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#LinearRegression -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearRegression">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel"/>
        <rdfs:comment>Regresja liniowa</rdfs:comment>
        <rdfs:label>Linear Regression</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#LogisticRegression -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#LogisticRegression">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel"/>
        <rdfs:comment>Regresja logistyczna do klasyfikacji</rdfs:comment>
        <rdfs:label>Logistic Regression</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#SVM -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#SVM">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel"/>
        <rdfs:comment>Support Vector Machine</rdfs:comment>
        <rdfs:label>SVM</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#InstanceBasedModel -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#InstanceBasedModel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <rdfs:comment>Modele oparte na instancjach</rdfs:comment>
        <rdfs:label>Instance-Based Model</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#KNN -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#KNN">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#InstanceBasedModel"/>
        <rdfs:comment>K-Nearest Neighbors</rdfs:comment>
        <rdfs:label>KNN</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#NeuralNetwork -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#NeuralNetwork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#LinearModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#TreeBasedModel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#InstanceBasedModel"/>
        <rdfs:comment>Sieci neuronowe</rdfs:comment>
        <rdfs:label>Neural Network</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#MLP -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#MLP">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#NeuralNetwork"/>
        <rdfs:comment>Multi-Layer Perceptron</rdfs:comment>
        <rdfs:label>MLP</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#CNN -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#CNN">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#NeuralNetwork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLP"/>
        <rdfs:comment>Convolutional Neural Network</rdfs:comment>
        <rdfs:label>CNN</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#RNN -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#RNN">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#NeuralNetwork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#MLP"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#CNN"/>
        <rdfs:comment>Recurrent Neural Network</rdfs:comment>
        <rdfs:label>RNN</rdfs:label>
    </owl:Class>

    <!-- Klasy pomocnicze (będą używane w relacjach) -->
    
    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#Dataset -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Dataset">
        <rdfs:comment>Zbiór danych (placeholder - będzie rozwinięty w części 2)</rdfs:comment>
        <rdfs:label>Dataset</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique">
        <rdfs:comment>Technika preprocessingu (placeholder - będzie rozwinięty w części 2)</rdfs:comment>
        <rdfs:label>Preprocessing Technique</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Normalization">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique"/>
        <rdfs:comment>Normalizacja danych</rdfs:comment>
        <rdfs:label>Normalization</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Standardization">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique"/>
        <rdfs:comment>Standaryzacja danych</rdfs:comment>
        <rdfs:label>Standardization</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#OneHotEncoding">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique"/>
        <rdfs:comment>Kodowanie One-Hot</rdfs:comment>
        <rdfs:label>One-Hot Encoding</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/ml-selection#Imputation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2024/ml-selection#PreprocessingTechnique"/>
